<h1>Listing turn_to_washes</h1>

<table>
  <thead>
    <tr>
      <th>Soldier</th>
      <th>DateTurn</th>
      <th>TurnToWash</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team.each do |Soldier| %>
      <tr>
        <td><%= soldier.organization %></td>
        <td><%= soldier.username %></td>
        <td><%= soldier.turn.first %></td>
               
        <td><%#= link_to 'Show', turn_to_wash %></td>
        <td><%#= link_to 'Edit', edit_turn_to_wash_path(turn_to_wash) %></td>
        <td><%#= link_to 'Destroy', turn_to_wash, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
