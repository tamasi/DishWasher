/************************************ FONT & BODY **************************************/

@import url(http://fonts.googleapis.com/css?family=Indie+Flower);

body {
  background: url(' <%= asset_path("texture.jpg") %> ');
  overflow-x: hidden;
}
a {
  color: $green;
}
a:hover, a:focus, a:active {
  color: $green-dark;
}
/************************************ HEADER   **************************************/
.logo-middle {
  margin: 0 auto;
  text-align: center;
  width: 100%;
}
header {
  margin-bottom: -10px;
}
.logo-middle img {
width: 700px;
}
.line {
  border-top:15px dotted $brown;
  left: 0;
  margin-top: -70px;
  overflow: hidden;
  position: absolute;
  width: 100%;
  z-index: -1;
}
#user_user_avatar {
  height: 45px;
  padding-top: 12px;
}
.page-wrapper {
  padding-top: 33px;
}
.container-user {
  padding-bottom: 32px
}
.container {
  margin-top: 3px;
}
.menu {
  padding-top: 5px !important;
  padding-bottom: 5px !important;
}
.navbar {
  margin-bottom: -40px;
  position: relative;
  z-index: 99;
  margin-top: 10px;
  margin-left: 80px;
}
.navbar-wrapper {
  margin-bottom: 10px;
}
.dropdown-menu {
  background-color: #fbf6f2; 
}
.dropdown-menu>li>a:hover, .dropdown-menu>li>a:focus {
  color: #5e4500;
  background-color: #fff;
}
.brown-menu, .brown-menu:hover {
  background-color: $brown !important;
  color: #fff;
}
.brown-menu:hover, .brown-menu:focus, .brown-menu:active {
  color: $brown-light !important;
}
/***************** CONTENT *********************/

.washer-list {
  list-style: none;
  margin-top: -42px;
}
.washer-list li {
  margin-bottom: 10px;
}
.img-rounded {
  border:3px solid $brown;
}
.today-washer {
  width: 100%;
}
.tomorrow-washer {
  width: 45%;
}
.u-are-next {
  background: url('<%= asset_path("arrow.png") %>') no-repeat left bottom;
  float: right;
  height: 95px;
  width: 50%;
}
.u-are-next p {
  font-family: 'Indie Flower', cursive;
  font-size: 20px;
}
.next-washer {
  width: 20%;
}
.hulk-said {

}
h1, h2, h3, h4, h5 {
  font-family: 'Indie Flower', cursive;
}
.bubble {
  background: url(' <%= asset_path("bubble.png") %> ') no-repeat top left;
  height: 219px;
  padding: 10px 0 0 60px;
  width: 400px;
}
.username {
  background-color: rgba(0,0,0,0.8);
  color: white;
  margin-top: -45px;
  padding-bottom: 2px;
  padding-right: 10px;
  padding-top: 4px;
  position: relative;
  text-align: right;
  width: 100%;
}
.date {
  background: url('<%= asset_path("calendary.png") %>') no-repeat;
  height: 214px;
  width: 200px;
}
.month {
  font-size: 40px;
  margin-bottom: 0;
  padding-bottom: 0;
  padding-left: 10px;
  padding-top: 42px;
  text-align: center;
  font-family: 'Indie Flower', cursive;
}
.day {
  font-size: 80px;
  text-align: center;
  padding-left: 15px;
  font-family: 'Indie Flower', cursive;
}
.pre-footer {
  background: url('<%= asset_path("texture-2.jpg") %>');
  border-top: 3px solid $brown-light;
  margin-top: 75px;
}
.divider-dish {
  background: url('<%= asset_path("divisor.png") %>') no-repeat center;
  border:none;
  display: block;
  height: 128px;
  margin:20px 0;
  text-align: center;
  position: relative;
  z-index:1;
  margin-top: -65px;
}
.clear {
  clear: both;
}
.pendant {
  background: url('<%= asset_path("pendant.png") %>') no-repeat top center;
  width: 100%;
  height: 100px;
  position: relative;
  z-index: 0;
  margin-bottom: -40px;
}
.week-days {
  font-size: 28px;
  font-family: 'Indie Flower', cursive;
}
.table-bordered {
  border: 2px solid #c3b06e;
  box-shadow: -3px 3px 7px #555;
  position: relative;
  z-index: 2;
}
.table-responsive {
  overflow: hidden;
  float: none;
  margin: 0 auto;
}
.whasher-list {
  width: 100px;
  height: 100px;
}
.header tr th {
  text-align: center;
  background-color: #fff;
}
.washer-list li a img:hover {
}
.date-user-list {
  display: block;
  font-size: 12px;
  line-height: 5px;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
tr.img-users-list td {
  text-align: center;
}
.breadcrumb {
  background: rgba(255,255,255,0.5);
}
/******* TO ACTIVE THE WASHER FOR TODAY *******/
.wash_today {
  background: #f85d5d !important;
  border: none !important;
  color: white;
}
tbody {
  border: none;
}
/******* CALENDAR *******/
#calendar {
  width: 100%;
  margin: 0 auto;
  }
.fc-view {
  background: #fff;
  box-shadow: -2px 10px 10px #666;
  -webkit-box-shadow: -2px 10px 10px #666;
  -moz-box-shadow: -2px 10px 10px #666;
  -o-box-shadow: -2px 10px 10px #666;
}
.angry-hulk {
  background: url('<%= asset_path("hulk-angry.gif") %>') no-repeat top center;
  width: 100%;
  height: 243px;
  position: relative;
  z-index: 1;
  margin-bottom: -49px;
}
.fc-header {
  position: relative;
  z-index: 2;
}

/********************************* LOGIN  **********************************/

.login-box {
  background: #fff;
  border-radius: 8px;
  -webkit-border-radius: 8px;
  -o-border-radius: 8px;
  -moz-border-radius: 8px;
  border:1px solid #eee;
  box-shadow: 2px 2px 5px #444;
  position: relative;
  top: 100px;
}
.into-title {
  padding-bottom: 10px;
  border-bottom: 1px solid #ccc;
}
.login-padding {
  padding-top: 10px;
}
.login-botons {
  padding:10px 0 20px 0;
}
.login-hulk {
  position: absolute;
  width: 100%;
  height: 100%;
  background: url('<%= asset_path("login-hulk.gif")%>') no-repeat top right;
  background-size: 100% 100%;
  z-index: -1;
  top: 0;
  right: 0;
  overflow: hidden;
  margin:0;
  padding: 0;
}

.edit-hulk {
  position: fixed;
  width: 100%;
  height: 100%;
  background: asset-url("login-hulk.gif") no-repeat top right;
  background-size: 100% 100%;
  z-index: -1;
  top: 0;
  right: 0;
  bottom: 2px;
  overflow: hidden;
  margin:0;
  padding: 0;
}

.table-hulk{
  position: fixed;
  width: 20%;
  height: 50%;
  background: url('<%= asset_path("bar-hulk.png") %>') no-repeat bottom right;
  background-size: 100% 100%;
  z-index: -1;
  top: 50%;
  right: 0;
  bottom: 2px;
  overflow: hidden;
  margin:0;
  padding: 0;
}
/***********************************  FOOTER  *************************************************/

footer .comp {
  float: left;
}
footer .dev{
  float: right
}

/******************************     ANIMATIONS    **********************************************/

.falling-2 {
  animation: fallToday linear 1s;
  animation-iteration-count: 1;
  -webkit-animation: fallToday linear 1s;
  -webkit-animation-iteration-count: 1;
  -moz-animation: fallToday linear 1s;
  -moz-animation-iteration-count: 1;
  -o-animation: fallToday linear 1s;
  -o-animation-iteration-count: 1;
  -ms-animation: fallToday linear 1s;
  -ms-animation-iteration-count: 1;
  position: relative;
}
@-webkit-keyframes fallToday {
  0% {
    top: -636px;
    -webkit-transform: rotate(0deg);
    -webkit-transform-origin: bottom left;
  }
  10% {
    -webkit-transform: rotate(0deg);
  }
  50% {
    top: -636px;
    -webkit-transform: rotate(90deg);
  }
  100% {
    top: 0;
    -webkit-transform: rotate(0deg);
  }
}
@-moz-keyframes fallToday {
  0% {
    top: -636px;
    -moz-transform: rotate(0deg);
    -moz-transform-origin: bottom left;
  }
  10% {
    -moz-transform: rotate(0deg);
  }
  50% {
    top: -636px;
    -moz-transform: rotate(90deg);
  }
  100% {
    top: 0;
    -moz-transform: rotate(0deg);
  }
}
@-o-keyframes fallToday {
  0% {
    top: -636px;
    -o-transform: rotate(0deg);
    -o-transform-origin: bottom left;
  }
  10% {
    -o-transform: rotate(0deg);
  }
  50% {
    top: -636px;
    -o-transform: rotate(90deg);
  }
  100% {
    top: 0;
    -o-transform: rotate(0deg);
  }
}
@keyframes fallToday {
  0% {
    top: -636px;
    transform: rotate(0deg);
    transform-origin: bottom left;
  }
  10% {
    transform: rotate(0deg);
  }
  50% {
    top: -636px;
    transform: rotate(90deg);
  }
  100% {
    top: 0;
    transform: rotate(0deg);
  }
}
/********************************************/
.falling-3 {
  -webkit-animation: fallTomorrow linear 1.5s;
  -webkit-animation-iteration-count: 1;
  -o-animation: fallTomorrow linear 1.5s;
  -o-animation-iteration-count: 1;
  -moz-animation: fallTomorrow linear 1.5s;
  -moz-animation-iteration-count: 1;
  animation: fallTomorrow linear 1.5s;
  animation-iteration-count: 1;
  position: relative;
}
@-webkit-keyframes fallTomorrow {
  0% {
    top: -636px;
    -webkit-transform: rotate(720deg);
    -webkit-transform-origin: bottom left;
  }
  50% {
    top: -636px;
    -webkit-transform: rotate(720deg);
  }
  100% {
    top: 0;
    -webkit-transform: rotate(0deg);
  }
}
@-o-keyframes fallTomorrow {
  0% {
    top: -636px;
    -o-transform: rotate(720deg);
    -o-transform-origin: bottom left;
  }
  50% {
    top: -636px;
    -o-transform: rotate(720deg);
  }
  100% {
    top: 0;
    -o-transform: rotate(0deg);
  }
}
@-moz-keyframes fallTomorrow {
  0% {
    top: -636px;
    -moz-transform: rotate(720deg);
    -moz-transform-origin: bottom left;
  }
  50% {
    top: -636px;
    -moz-transform: rotate(720deg);
  }
  100% {
    top: 0;
    -moz-transform: rotate(0deg);
  }
}
@keyframes fallTomorrow {
  0% {
    top: -636px;
    transform: rotate(720deg);
    transform-origin: bottom left;
  }
  50% {
    top: -636px;
    transform: rotate(720deg);
  }
  100% {
    top: 0;
    transform: rotate(0deg);
  }
}
/********************************************/
.falling-next-2, .falling-next-1, .falling-next-0 {
  -webkit-animation-iteration-count: 1;
  -o-animation-iteration-count: 1;
  -moz-animation-iteration-count: 1;
  animation-iteration-count: 1;
  position: relative;
}
.falling-next-2, .falling-next-1, .falling-next-0  {
  -webkit-animation: fallNextOne linear 2s;
  -o-animation: fallNextOne linear 2s;
  -moz-animation: fallNextOne linear 2s;
  animation: fallNextOne linear 2s;
}
@-webkit-keyframes fallNextOne {
  0% {
    -webkit-transform: scale(0);
  }
  80% {
    -webkit-transform: scale(0);
  }
  90% {
    -webkit-transform: scale(1.2);
  }
  100% {
    -webkit-transform: scale(1);
  }
}
@-o-keyframes fallNextOne {
  0% {
    -o-transform: scale(0);
  }
  80% {
    -o-transform: scale(0);
  }
  90% {
    -o-transform: scale(1.2);
  }
  100% {
    -o-transform: scale(1);
  }
}
@-moz-keyframes fallNextOne {
  0% {
    -moz-transform: scale(0);
  }
  80% {
    -moz-transform: scale(0);
  }
  90% {
    -moz-transform: scale(1.2);
  }
  100% {
    -moz-transform: scale(1);
  }
}
@keyframes fallNextOne {
  0% {
    transform: scale(0);
  }
  80% {
    transform: scale(0);
  }
  90% {
    transform: scale(1.2);
  }
  100% {
    transform: scale(1);
  }
}
@-webkit-keyframes fallNextTwo {
  0% {
    -webkit-transform: scale(0);
  }
  84% {
    -webkit-transform: scale(0);
  }
  94% {
    -webkit-transform: scale(1.2);
  }
  100% {
    -webkit-transform: scale(1);
  }
}
@-o-keyframes fallNextTwo {
  0% {
    -o-transform: scale(0);
  }
  84% {
    -o-transform: scale(0);
  }
  94% {
    -o-transform: scale(1.2);
