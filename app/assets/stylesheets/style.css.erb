#delay-hulk {
  -webkit-animation-duration: .5s;
  -webkit-animation-delay: 1.5s;
  -o-animation-duration: .5s;
  -o-animation-delay: 1.5s;
  -moz-animation-duration: .5s;
  -moz-animation-delay: 1.5s;
  animation-duration: .5s;
  animation-delay: 1.5s;
}
#delay-calendar {
  -webkit-animation-duration: 1s;
  -webkit-animation-delay: 1.5s;
  -o-animation-duration: 1s;
  -o-animation-delay: 1.5s;
  -moz-animation-duration: 1s;
  -moz-animation-delay: 1.5s;
  animation-duration: 1s;
  animation-delay: 1.5s;
}
.logo-middle {
  -webkit-animation-duration: 1s;
  -webkit-animation-delay: 2s;
  -o-animation-duration: 1s;
  -o-animation-delay: 2s;
  -moz-animation-duration: 1s;
  -moz-animation-delay: 2s;
  animation-duration: 1s;
  animation-delay: 2s;
}
.navbar-nav {
  -webkit-animation-duration: 1s;
  -webkit-animation-delay: 2.5s;
  -o-animation-duration: 1s;
  -o-animation-delay: 2.5s;
  -moz-animation-duration: 1s;
  -moz-animation-delay: 2.5s;
  animation-duration: 1s;
  animation-delay: 2.5s;
}

/****************************************** my-turn page ********************************/

.secondary {
  -webkit-animation-delay: 0.5s !important;
  -o-animation-delay: 0.5s !important;
  -moz-animation-delay: 0.5s !important;
  animation-delay: 0.5s !important;
}
.angry-hulk {
  -webkit-animation-duration: 1s;
  -webkit-animation-delay: 3s;
  -o-animation-duration: 1s;
  -o-animation-delay: 3s;
  -moz-animation-duration: 1s;
  -moz-animation-delay: 3s;
  animation-duration: 1s;
  animation-delay: 3s;
}
.responsive-username-hide, .responsive-logo {
  display: none;
}
.mobile-nav {
  display: none
}
.single-nav {
  display: block;
}
/****************************************** RESPONSIVE MODE ********************************/

@media(max-width: 1025px) {
  .whasher-list {
    width: 100%;
    height: auto;
  }
}

@media(max-width: 769px) {
  ul.washer-list {
    padding-left: 0;
  }
  .responsive-mode {
    width: 100%;
  }
  .bubble {
    height: 180px;
    padding: 5px 0 0 42px;
    width: 329px;
    background-size: 100% 100%;
  }
  #delay-calendar {
    padding: 0;
  }
  .date {
    height: 182px;
    width: 170px;
    background-size: 100% 100%;
  }
  .month {
    padding-top: 24px;
  }
  .hulk-said {
    width: 140%
  }
  .u-are-next {
    height: 75px;
  }
  .u-are-next p {
    font-size: 14px
  }
  .bubble h3 {
    margin-top: 0;
  }
}


@media (max-width:320px) {
  .navbar {
    margin-top: 0;
  }
  .mobile-nav {
    display: block
  }
  .single-nav {
    display: none;
  }
  .navbar {
    background-color: $brown;
    margin: 0px;
  }
  .navbar-toggle {
    background-color: $brown-light;
    color: #fff;
  }
  .navbar-toggle .icon-bar {
    background-color: #fff;
  }
  .table-responsive {
    border: none;
  }
  .navbar-nav li a {
    text-transform: uppercase;
    color: $brown-light;
  }
  header {
    margin-top: 8px;
  }
  .logo-middle img {
    width: 80%;
  }
  .line {
    margin-top: 0;
    border-top: 8px dotted #664303;
  }
  .washer-list {
    margin-top: 30px;
  }
  .washer-list li {
    float: left;
    width: 60px;
    margin: 0 2px;
  }
  .washer-list li:last-child {
    width: 100%;
  }
  .washer-list li p {
    text-align: center;
  }
  .next-washer, .tomorrow-washer {
    float: left;
    display: inline-block;
    width: 100%;
  }
  .u-are-next, #delay-hulk, #delay-calendar {
    display: none
  }
  .responsive-username-hide {
    display: block;
  }
  .logo-home {
    display: none;
  }
  .responsive-logo {
    display: block;
  }
  .pendant {
    margin-bottom: -70px;
  }
  .week-days {
    font-size: 17px;
  }
  .date-user-list {
    font-size: 10px;
  }
  footer {
    margin-top: 20px;
  }
  footer .comp, footer .dev {
    font-size: 10px
  }
}